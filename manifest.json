{
   "browser_action": {
      "default_icon": "icon48.png",
      "default_title": "TabPlus",
      "popup": "popup.html"
   },
   "background_page": "background.html",
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "keyboard_handler.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Lets you manipulate tabs easily",
      "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
   },
   "name": "Tab plus",
   "permissions": [ "tabs", "http://*/*", "https://*/*" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "0.7.2"
}
